{
	"name" : "coka/rate-limit-bundle",
	"type" : "symfony-bundle",
	"license" : "MIT",
	"description" : "Request rate limit",
	"keywords" : [
		"rate",
		"limit"
	],
	"authors" : [{
			"name" : "Cedrick Oka Baidai",
			"email" : "okacedrick@gmail.com"
		}
	],
	"autoload" : {
		"psr-4" : {
			"Oka\\RateLimitBundle\\" : "src"
		}
	},
	"require" : {
		"php" : ">=7.2",
		"symfony/cache" : "^3.4 || ^4.4",
		"symfony/config" : "^3.4 || ^4.4",
		"symfony/dependency-injection" : "^3.4 || ^4.4",
		"symfony/http-kernel" : "^3.4 || ^4.4",
		"coka/rest-request-validator-bundle" : "^1.1",
		"symfony/security-bundle": "^3.4 || ^4.4"
	},
	"extra" : {
		"branch-alias" : {
			"dev-master" : "1.x.x-dev"
		},
		"symfony" : {
			"allow-contrib" : "true"
		}
	},
	"support" : {
		"docs" : "https://github.com/CedrickOka/rate-limit-bundle/blob/master/src/Resources/doc/index.md"
	}
}
